<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.geometry.Insets?>
<VBox fx:controller="ru.kpfu.itis.gnt.hwchat.client.controllers.RegistrationController" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="180.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets top="10" left="10" right="10" bottom="10"/>
   </padding>
   <children>
      <Label text="Register" fx:id="titleLabel" />
      <Region VBox.vgrow="ALWAYS" />
      <TextField fx:id="registerTextField" promptText="Enter your username"/>
      <Region prefHeight="10" />
      <Button fx:id="registerButton" mnemonicParsing="false" prefHeight="26.0" onMouseClicked="#onSubmitClicked" prefWidth="300.0" text="Submit" />
   </children>
</VBox>
